<div class="login_container">
    <div class="land teal">
        <div class="center white-text">
            <h4>PHARMA SOFTWARE</h4>
        </div>
    </div>
    <div class="row logger">
        <div class="col m3"></div>
        <div class="col m6">
            <div class="card grey darken-4 no_margin login_title">
                <h5 class="no_margin">Register</h5>
            </div>
            <div class="card no_margin login_body">
                <div class="row">
                    <form class="col s12" method="POST" action="/Register/Saving">
                        <div class="row no_margin">
                            <div class="input-field col s12">
                                <input id="fullname" required type="text" name="fullname" class="validate">
                                <label for="fullname">Full Name</label>
                            </div>
                            <div class="input-field col s12">
                                <input id="email" required type="email" name="email" class="validate">
                                <label for="email">Email</label>
                            </div>
                            <div class="input-field col s12">
                                <input id="password" required type="password" name="password" class="validate">
                                <label for="password">Password</label>
                            </div>
                            <div class="col s12">
                                <p class="center">
                                    <input type="submit" class="waves-effect waves-light btn grey darken-4" value="Register"/>
                                    <input type="reset" class="waves-effect waves-light btn red darken-4" value="Clear"/>
                                </p>
                                <p class="center">
                                    <a href="../Login" class="waves-effect waves-light btn teal darken-3">Already have an account?</a>
                                </p>
                            </div>
                            <div class="col m12 center mgt">
                                <h6><a href="#">Forgot your password?</a></h6>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
        <div class="col m3"></div>
    </div>
</div>
